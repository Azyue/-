/* Adminpackage/miniApp/miniApp.wxss */
/* 功能列表 */
.sudoku_list {
	width: 100%;
	height: auto;
	display: flex;
	flex-wrap: wrap;
	flex-direction: row;
	justify-content: center;
	background-color: #fff;
}

.sudoku_item {
	width: 15%;
	margin: 10rpx;
}

.sudoku_item_name {
	margin: 10rpx;
	text-align: center;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
}

.sudoku_item_img {
	width: 100rpx;
	height: 100rpx;
	border-radius: 16px;
	overflow: hidden;
}